let m_config={type:'entity',entity:"sensor.rsdose4_338229039_dosing_queue"}
let s_config={type:'entity',entity:"switch.rsdose4_338229039_device_state"};
const SensorRow = customElements.get('hui-sensor-entity-row')
const sensor = new SensorRow()
sensor.setConfig(m_config)
sensor.hass=this.hass;
const SRow = customElements.get('hui-toggle-entity-row')
const ssor = new SRow()
ssor.setConfig(s_config)
ssor.hass=this.hass;
hmlt` ${sensor} ${ssor}`;
