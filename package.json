{
  "name": "ha-reef-card",
  "version": "1.0.0",
  "type": "module",
  "description": "Home Assistant Reef Tank Card - TypeScript Strict Version",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "watch": "vite build --watch",
    "watch:prod": "vite build --watch --mode production",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.0",
    "prettier": "^3.3.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "lit": "^3.3.1"
  },
  "keywords": [
    "home-assistant",
    "custom-card",
    "reef-tank",
    "aquarium",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
